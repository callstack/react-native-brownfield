{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build:tester-integrated:android": {
      "dependsOn": ["build:brownfield"],
      "env": ["JAVA_HOME", "ANDROID_NDK", "ANDROID_SDK", "ANDROID_HOME"],
      "inputs": [
        "apps/TesterIntegrated/package.json",
        "apps/TesterIntegrated/kotlin",
        "!apps/TesterIntegrated/kotlin/.gradle",
        "!apps/TesterIntegrated/kotlin/build",
        "!apps/TesterIntegrated/kotlin/app/build"
      ],
      "outputs": []
    },
    "build:tester-integrated:ios": {
      "dependsOn": ["build:brownfield"],
      "env": ["RCT_NEW_ARCH_ENABLED"],
      "inputs": [
        "apps/TesterIntegrated/package.json",
        "apps/TesterIntegrated/swift",
        "!apps/TesterIntegrated/swift/build",
        "!apps/TesterIntegrated/swift/Pods"
      ],
      "outputs": []
    }
  }
}
