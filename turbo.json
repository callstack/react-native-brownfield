{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {
      "dependsOn": ["build:brownfield"],
      "inputs": ["**/*.ts", "**/*.tsx", "**/*.js"]
    },
    "typecheck": {
      "dependsOn": ["build:brownfield"],
      "inputs": ["**/*.ts", "**/*.tsx"]
    },
    "test": {},
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
