{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "types": ["node", "jest"]
  },
  "include": [
    "**/*.ts",
    "../__fixtures__/**/*.ts",
    "../*.ts",
    "../commands/*.ts",
    "../generators/*.ts"
  ]
}
